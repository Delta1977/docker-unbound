{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:recommended"
  ],
  "schedule": [
    "* 0-3 * * *"
  ],



  "customDatasources": {
    "unbound": {
      "defaultRegistryUrlTemplate": "https://nlnetlabs.nl/downloads/unbound/",
      "format": "html"
    }
  },
  "packageRules": [
    {
      "matchDatasources": ["custom.unbound"],
      "extractVersion": "^unbound-(?<version>.+)\\.tar\\.gz$"
    }
  ]

}
